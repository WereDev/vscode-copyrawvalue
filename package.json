{
	"name": "copyrawvalue",
	"displayName": "Copy Raw Value",
	"description": "Copies unescaped value in Run - Variables.",
	"version": "1.0.0",
	"publisher": "weredev",
	"icon": "icon.png",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:variables.copyraw"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "variables.copyraw",
				"title": "Copy Raw Value"
			}
		],
		"menus": {
			"debug/variables/context": [
				{
					"when": "true",
					"command": "variables.copyraw",
					"group": "5_cutcopypaste@1"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-debugadapter": "^1.43.0",
		"vscode-debugprotocol": "^1.43.0",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {}
}
